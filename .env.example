# LangSmith Configuration
# Copy this file to .env and fill in your actual API key

# Enable LangSmith tracing (set to "true" to enable, "false" or remove to disable)
LANGCHAIN_TRACING_V2=true

# Your LangSmith API key from https://smith.langchain.com/
# Get your key from: Settings â†’ API Keys
LANGCHAIN_API_KEY=lsv2_pt_your_api_key_here

# Project name in LangSmith (must match your project in the dashboard)
LANGCHAIN_PROJECT=care-assistant

# LangSmith API endpoint (default, usually no need to change)
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Optional: Environment tag for organizing traces
ENVIRONMENT=development
